class Employee{
	
	String name;
	int empID;
	
	public Employee() {
		this("name", 0);
		System.out.println("Hello there");
	}
	public Employee(String name,int empID) {
		this.name=name;
		this.empID=empID;
	}
}
public class Constructor {

	public static void main(String[] args) {
		
		Employee e1= new Employee();
		System.out.println(e1.name);

	}

}
