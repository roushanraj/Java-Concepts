class Animal{
	public String name;
	public static String type;
}
public class StaticKeyword {

	public static void main(String[] args) {
		Animal.type="vertebrate";
		System.out.println(Animal.type);
		
		Animal dogs= new Animal();
		dogs.name="Dog";
		dogs.type="vertebra";
		System.out.println(dogs.type);
		
		Animal cats= new Animal();
		cats.name="Cat";
		System.out.println(cats.type);
		

	}

}
